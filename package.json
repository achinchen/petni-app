{
  "name": "petni",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && cross-env NODE_ENV=production remix build",
    "build:css": "unocss './app/**/*.{ts,tsx}' -o ./app/styles/uno.css",
    "dev": "concurrently 'npm run dev:css' 'npm run dev:remix' ",
    "dev:remix": "dotenv -e .env.local cross-env NODE_ENV=development remix dev",
    "dev:css": "unocss './app/**/*.{ts,tsx}' -o ./app/styles/uno.css --watch",
    "dev:seed": "node --require esbuild-register ./database/seed/dev.ts",
    "postinstall": "remix setup node",
    "img:album": "node --require esbuild-register ./app/utils/imgur/album/index.server.ts ",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "test:db": "dotenv -e .env.test prisma db push",
    "test:seed": "NODE_ENV=test node --require esbuild-register ./database/seed/test.ts",
    "test:integrate": "NODE_ENV=test yarn docker:up && yarn test:db && yarn test:seed && jest -i",
    "test": "NODE_ENV=test jest -i"
  },
  "dependencies": {
    "@lottiefiles/lottie-player": "^1.5.6",
    "@prisma/client": "^4.1.0",
    "@remix-run/react": "^1.5.1",
    "@remix-run/vercel": "^1.5.1",
    "debounce": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.5.1",
    "remix-auth": "^3.2.2",
    "remix-auth-oauth2": "^1.2.2"
  },
  "devDependencies": {
    "@babel/helper-string-parser": "^7.18.10",
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "@testing-library/react": "^12.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.1",
    "@types/debounce": "^1.2.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.7.16",
    "@types/node-fetch": "2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@unocss/preset-attributify": "^0.34.1",
    "@unocss/preset-web-fonts": "^0.34.1",
    "@unocss/reset": "^0.34.1",
    "@unocss/transformer-directives": "^0.34.1",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "jest-mock-extended": "^2.0.5",
    "kebab-case": "^1.0.1",
    "node-fetch": "2",
    "prettier": "^2.5.1",
    "prisma": "^4.1.0",
    "ts-jest": "^29.0.0-next.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5",
    "unocss": "^0.28.1"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "schema": "./database/schema.prisma"
  }
}
